<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.home.NaviHomeFragment">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraint_new_gym"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginEnd="5dp"
            android:layout_marginBottom="18dp"
            android:layout_marginTop="100dp"
            android:background="@drawable/background_bluegrey"
            android:paddingBottom="20dp">

            <TextView
                android:id="@+id/gym_name_textview"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:layout_marginTop="10dp"
                android:text="신축"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/gym_current_user_textview"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="현재 인원 : "
                app:layout_constraintStart_toStartOf="@id/gym_name_textview"
                app:layout_constraintTop_toBottomOf="@+id/gym_name_textview" />

            <TextView
                android:id="@+id/gym_current_user_value_textview"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="@id/gym_current_user_textview"
                app:layout_constraintStart_toEndOf="@+id/gym_current_user_textview"
                app:layout_constraintTop_toTopOf="@+id/gym_current_user_textview"
                tools:text="15" />


        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraint_layout_yeasol_gym"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginEnd="5dp"
            android:layout_marginBottom="18dp"
            android:layout_marginTop="250dp"
            android:background="@drawable/background_bluegrey"
            android:paddingBottom="20dp">

            <TextView
                android:id="@+id/gym_name_textview2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:layout_marginTop="10dp"
                android:text="예솔"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/gym_current_user_textview2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="현재 인원 : "
                app:layout_constraintStart_toStartOf="@id/gym_name_textview2"
                app:layout_constraintTop_toBottomOf="@+id/gym_name_textview2" />

            <TextView
                android:id="@+id/gym_current_user_value_textview2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="@id/gym_current_user_textview2"
                app:layout_constraintStart_toEndOf="@+id/gym_current_user_textview2"
                app:layout_constraintTop_toTopOf="@+id/gym_current_user_textview2"
                tools:text="15" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraint_layout_damheon_gym"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginEnd="5dp"
            android:layout_marginBottom="18dp"
            android:layout_marginTop="400dp"
            android:background="@drawable/background_bluegrey"
            android:paddingBottom="20dp">

            <TextView
                android:id="@+id/gym_name_textview3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:layout_marginTop="10dp"
                android:text="담헌"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/gym_current_user_textview3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="현재 인원 : "
                app:layout_constraintStart_toStartOf="@id/gym_name_textview3"
                app:layout_constraintTop_toBottomOf="@+id/gym_name_textview3" />

            <TextView
                android:id="@+id/gym_current_user_value_textview3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="@id/gym_current_user_textview3"
                app:layout_constraintStart_toEndOf="@+id/gym_current_user_textview3"
                app:layout_constraintTop_toTopOf="@+id/gym_current_user_textview3"
                tools:text="15" />


        </androidx.constraintlayout.widget.ConstraintLayout>

    </FrameLayout>
</layout>